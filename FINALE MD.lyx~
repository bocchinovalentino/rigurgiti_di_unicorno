\begin_inset Formula $-22\cdot($
\end_inset


\color orange

\begin_inset Formula $100$
\end_inset


\color inherit

\begin_inset Formula $-1\cdot$
\end_inset


\color blue

\begin_inset Formula $71$
\end_inset


\color inherit

\begin_inset Formula $)+9\cdot$
\end_inset


\color blue

\begin_inset Formula $71$
\end_inset


\color inherit

\begin_inset Formula $=$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\phantom{1}=$
\end_inset


\begin_inset Formula $-22\cdot$
\end_inset


\color orange

\begin_inset Formula $100$
\end_inset


\color inherit

\begin_inset Formula $+22\cdot$
\end_inset


\color blue

\begin_inset Formula $71$
\end_inset


