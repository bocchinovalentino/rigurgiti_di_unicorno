\begin_inset Formula 
\[
\dbinom{7}{2}=\dfrac{7!}{2!\cdot(7-2)!}=21
\]

\end_inset


\family default
\series default
\shape default
\size default
\emph default
\nospellcheck default
\bar default
\strikeout default
\xout default
\uuline default
