\end_inset


\color blue

\begin_inset Formula $3$
\end_inset


\color inherit

\begin_inset Formula $+$
\end_inset


\color cyan

\begin_inset Formula $1$
\end_inset


\color blue

\begin_inset Newline newline
\end_inset


\begin_inset Formula $3$
\end_inset


\color inherit

\begin_inset Formula $=3\cdot$
\end_inset


\color cyan

\begin_inset Formula $1$
\end_inset


\color inherit

\begin_inset Formula $+$
\end_inset


\color teal

\begin_inset Formula $0$
\end_inset


\color cyan

