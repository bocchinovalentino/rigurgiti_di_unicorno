\end_inset


\end_layout

\begin_layout Subsubsection
Divisione euclidea
\end_layout

\begin_layout Standard
Calcolare la divisione euclidea per le seguenti coppie di dividendo 
\begin_inset Formula $a$
\end_inset

 e divisore 
\begin_inset Formula $b$
\end_inset

:
\end_layout

\begin_layout Enumerate
\begin_inset Formula $a=26.754,b=-307$
\end_inset


\end_layout

\begin_layout Enumerate

\color red
\begin_inset Formula $a=-29.244,b=289$
\end_inset


\end_layout

\begin_layout Enumerate
\begin_inset Formula $a=781.116,b=1.101$
\end_inset


\end_layout

\begin_layout Standard
Soluzione (
\emph on

\begin_inset Formula $q$
\end_inset

 è il quoziente ed 
\begin_inset Formula $r$
\end_inset

 è il resto
\emph default
) :
\end_layout

\begin_layout Standard
\align center
\begin_inset Tabular
<lyxtabular version="3" rows="1" columns="3">
<features tabularvalignment="middle">
<column alignment="left" valignment="middle">
<column alignment="left" valignment="middle">
<column alignment="left" valignment="middle">
<row>
<cell alignment="left" valignment="middle" usebox="none">
\begin_inset Text

\begin_layout Enumerate
\begin_inset Formula $q=-87,r=45$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="left" valignment="middle" usebox="none">
\begin_inset Text

\begin_layout Enumerate
\begin_inset Argument 1
status collapsed

\begin_layout Plain Layout
\begin_inset ERT
status open

\begin_layout Plain Layout

start=2
\end_layout

\end_inset


\end_layout

\end_inset


\color red

\begin_inset Formula $q=-102,r=234$
