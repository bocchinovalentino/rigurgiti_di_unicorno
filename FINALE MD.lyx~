reference "subsec:Calcolo-del-MCD"
plural "false"
caps "false"
noprefix "false"
nolink "false"

\end_inset

 e 
\begin_inset CommandInset ref
LatexCommand ref
reference "subsec:Teorema:-Identità-Bezout"
plural "false"
caps "false"
